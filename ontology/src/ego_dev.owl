    <!-- http://purl.obolibrary.org/obo/EGO_0000000008 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/EGO_0000000008">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
